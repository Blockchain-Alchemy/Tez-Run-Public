{
  "name": "teo-run-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@taquito/beacon-wallet": "^10.2.1",
    "@taquito/taquito": "^10.2.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "express": "^4.17.3",
    "moment": "^2.29.1",
    "qrcode-generator": "^1.4.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "react-transition-group": "^4.4.2",
    "react-unity-webgl": "^8.7.4",
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "start": "set PORT=3004 && concurrently \"react-scripts start\" \"npm run tailwind-watch\"",
    "build": "npm run tailwind && react-scripts build",
    "tailwind": "tailwind -i ./src/assets/tailwind.css -c tailwind.config.js -o ./src/assets/styles.css",
    "tailwind-watch": "npm run tailwind --watch",
    "heroku": "node ./server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-taquito": "ncu -u --target newest --filter '/^@taquito.*$/' && npm i"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.4"
  }
}
